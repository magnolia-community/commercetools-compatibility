<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd" >
<module>
  <name>commercetools-demo</name>
  <displayName>${project.name}</displayName>
  <description>${project.description}</description>
  <versionHandler>info.magnolia.commercetools.demo.setup.CommercetoolsDemoModuleVersionHandler</versionHandler>
  <version>${project.version}</version>

  <properties>
    <property>
      <name>defaultPublicURI</name>
      <value>redirect:/commercetools.html</value>
    </property>
  </properties>

  <dependencies>
    <dependency>
      <name>commercetools-integration</name>
      <version>${project.version}/*</version>
    </dependency>

    <dependency>
      <name>commercetools-templates</name>
      <version>${project.version}/*</version>
    </dependency>

    <dependency>
      <name>travel-demo</name>
      <version>1.2/*</version>
      <optional>true</optional>
    </dependency>

    <dependency>
      <name>multisite</name>
      <version>1.3/*</version>
      <optional>true</optional>
    </dependency>
  </dependencies>
</module>
